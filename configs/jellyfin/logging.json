{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"Microsoft": "Debug",
				"System": "Debug"
			}
		},
		"WriteTo:Async": {
			"Name": "Async",
			"Args": {
				"configure": [
					{
						"Name": "File",
						"Args": {
							"fileSizeLimitBytes": null,
							"outputTemplate": "[{Timestamp:yyyy-MM-ddTHH:mm:ss.fffzzz}] [{Level:u1}] {SourceContext}: {Message:j}{NewLine}{NewLine}",
							"path": "%JELLYFIN_LOG_DIR%//debrids.log",
							"retainedFileCountLimit": 1,
							"rollingInterval": "Infinite",
							"rollOnFileSizeLimit": false
						}
					}
				]
			}
		}
	}
}
